{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": []
}
{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-DNS-Prefetch-Control", "value": "on" },
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/:path*", "destination": "https://your-blog.vercel.app/:path*" }
  ]
}
